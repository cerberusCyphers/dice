teX(-50%);\n  color: #444;\n  background: none;\n  border: none;\n  font-family: inherit;\n  font-size: 1.8rem;\n  text-transform: uppercase;\n  cursor: pointer;\n  font-weight: 400;\n  transition: all 0.2s;\n\n  background-color: white;\n  background-color: rgba(255, 255, 255, 0.6);\n  backdrop-filter: blur(10px);\n\n  padding: 0.7rem 2.5rem;\n  border-radius: 50rem;\n  box-shadow: 0 1.75rem 3.5rem rgba(0, 0, 0, 0.1);\n}\n\n.btn::first-letter {\n  font-size: 2.4rem;\n  display: inline-block;\n  margin-right: 0.7rem;\n}\n\n.btn--new {\n  top: 4rem;\n}\n.btn--roll {\n  top: 39.3rem;\n}\n.btn--hold {\n  top: 46.1rem;\n}\n\n.btn:active {\n  transform: translate(-50%, 3px);\n  box-shadow: 0 1rem 2rem rgba(0, 0, 0, 0.15);\n}\n\n.btn:focus {\n  outline: none;\n}\n\n.dice {\n  position: absolute;\n  left: 50%;\n  top: 16.5rem;\n  transform: translateX(-50%);\n  height: 10rem;\n  box-shadow: 0 2rem 5rem rgba(0, 0, 0, 0.2);\n}\n\n.player--winner {\n  background-image: url(./img/trophy.jfif);\n  background-size: cover;\n}\n\n.player--winner .name {\n  font-weight: 700;\n  color: #ff0000;\n}\n\n.hidden {\n  display: none;\n}\n"},"lineCount":null}},"error":null,"hash":"2e8f071a28f4d349fbb2792db26bfc48","cacheData":{"env":{}}}